{
  "name": "tornado-ui",
  "version": "2.6.0",
  "description": "Tornado is an Open Source Framework for Forntend and UI/UX Designers made with love from a designer to designers the Main Goal of this framework is to make web design more simple and providing the best performance and dynamic easy to control elements Builded With Sass for Easy Customizing and Overriding Every Thing.",
  "main": "/dist/js/tornado.min.js",
  "exports": {
    "tornado-ts": "./src/typescript/",
    "tornado-sass": "./src/SCSS/",
    "tornado-js": "./dist/js/",
    "tornado-css": "./dist/css/"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "tornado-ts": "npx webpack --config typescript.webpack.js --watch",
    "tornado-sass": "npx node-sass --watch src/sass -o dist/css --output-style compressed --source-map true",
    "tornado-start": "concurrently --kill-others \"npm run tornado-sass\" \"npm run tornado-ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EngCode/Tornado-v2.git"
  },
  "author": "Abdullah.Ramadan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EngCode/Tornado-v2/issues"
  },
  "homepage": "https://tornado.phenixthemes.com",
  "dependencies": {
    "tiny-slider": "^2.9.4",
    "concurrently": "^6.4.0",
    "node-sass": "^7.0.1",
    "ts-loader": "^9.4.2",
    "typescript": "^3.9.7",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.0.0"
  },
  "devDependencies": {},
  "keywords": [
    "tornado ui",
    "framework",
    "css",
    "js",
    "esNext",
    "es6",
    "react",
    "wordpress",
    "design",
    "web",
    "app",
    "enviroment",
    "webpack",
    "sass",
    "rtl",
    "ux",
    "typescript",
    "components"
  ]
}
